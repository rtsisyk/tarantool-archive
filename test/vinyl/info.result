env = require('test_run')
---
...
test_run = env.new()
---
...
test_run:cmd('create server vinyl_info with script="vinyl/vinyl_info.lua"')
---
- true
...
test_run:cmd("start server vinyl_info")
---
- true
...
test_run:cmd('switch vinyl_info')
